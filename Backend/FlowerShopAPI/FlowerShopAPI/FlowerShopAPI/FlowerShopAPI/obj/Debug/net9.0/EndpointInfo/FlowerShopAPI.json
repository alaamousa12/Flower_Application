    "/api/Auth/register": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "Email",
                  "Name",
                  "Password",
                  "PhoneNumber"
                ],
                "type": "object",
                "properties": {
                  "Name": {
                    "type": "string"
                  },
                  "Email": {
                    "type": "string",
                    "format": "email"
                  },
                  "PhoneNumber": {
                    "type": "string"
                  },
                  "Password": {
                    "minLength": 8,
                    "pattern": "^(?=.*[A-Z])(?=.*[!@#$%^&*(),.?\"{}|<>]).*$",
                    "type": "string"
                  },
                  "Gender": {
                    "type": "string"
                  },
                  "Country": {
                    "type": "string"
                  },
